{
  "name": "collaborative-editor",
  "version": "0.0.1",
  "description": "Learn collaborative softwares by creating a collaborative editor",
  "scripts": {
    "build": "rimraf ./dist && cross-env NODE_ENV=production rollup -c",
    "build-dev": "rimraf ./dist && cross-env NODE_ENV=development rollup -c",
    "start": "node dist/index.js",
    "dev": "ts-node scripts/development",
    "test": "jest",
    "prepare": "husky install",
    "format": "prettier --write --ignore-unknown ./src/**/*.ts",
    "lint": "eslint --ext .ts src/"
  },
  "keywords": [
    "collaborative",
    "editor"
  ],
  "author": "nc_8093@163.com",
  "license": "MIT",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "ot-json1": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "reconnecting-websocket": "^4.4.0",
    "sharedb": "^3.3.0",
    "slate": "^0.82.1",
    "slate-react": "^0.82.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/sharedb": "^3.3.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{ts, js}": [
      "eslint",
      "prettier --write",
      "jest --bail --findRelatedTests --passWithNoTests"
    ],
    "*.{json,md,yaml,yml,css}": "prettier --write"
  }
}
