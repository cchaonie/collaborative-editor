{
  "name": "collaborative-editor",
  "version": "0.0.1",
  "description": "Learn collaborative softwares by creating a collaborative editor",
  "scripts": {
    "build": "rimraf ./dist && cross-env NODE_ENV=production rollup -c",
    "build-dev": "rimraf ./dist && cross-env NODE_ENV=development rollup -c",
    "start": "node dist/index.js",
    "dev": "ts-node scripts/development",
    "test": "jest",
    "prepare": "husky install",
    "format": "prettier --write --ignore-unknown ./src/**/*.ts",
    "lint": "eslint src/"
  },
  "keywords": [
    "collaborative",
    "editor"
  ],
  "author": "nc_8093@163.com",
  "license": "MIT",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "ot-json1": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "reconnecting-websocket": "^4.4.0",
    "sharedb": "^3.3.2",
    "slate": "^0.82.1",
    "slate-react": "^0.82.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-alias": "^4.0.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/sharedb": "^3.3.9",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.33",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{ts, js}": [
      "eslint",
      "prettier --write",
      "jest --bail --findRelatedTests --passWithNoTests"
    ],
    "*.{json,md,yaml,yml,css}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "bl@<0.9.5": ">=0.9.5",
      "bl@<1.2.3": ">=1.2.3",
      "semver@<4.3.2": ">=4.3.2",
      "browserify-sign@>=2.6.0 <=4.2.1": ">=4.2.2",
      "postcss@<8.4.31": ">=8.4.31",
      "yaml@>=2.0.0-5 <2.2.2": ">=2.2.2",
      "word-wrap@<1.2.4": ">=1.2.4",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2"
    }
  }
}
