{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "strict": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "~client/*": [
        "src/client/*"
      ],
      "~handler/*": [
        "src/handler/*"
      ],
      "~middleware/*": [
        "src/middleware/*"
      ],
      "~util/*": [
        "src/util/*"
      ]
    }
  },
}